          <HBox>
            <AnchorPane>
               <children>
                    <ImageView fx:id="imageView" fitHeight="142.0" fitWidth="141.0" layoutX="7.0" layoutY="-25.0" opacity="0.4" pickOnBounds="true" preserveRatio="true">
